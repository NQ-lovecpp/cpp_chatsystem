{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Examples/registry",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/Examples",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "internalConsoleOptions": "openOnSessionStart",
            "logging": {
                "engineLogging": true
            },
            "launchCompleteCommand": "exec-run",
            "debugServerPath": "",
            "debugServerArgs": "",
            "serverStarted": "",
            "filterStderr": false,
            "filterStdout": false,
            "showDisplayString": true,
            "postRemoteConnectCommands": [],
            "remoteRoot": "",
            "osx": {
                "MIMode": "lldb"
            },
            "linux": {
                "MIMode": "gdb"
            },
            "windows": {
                "MIMode": "gdb"
            }
        }
    ]
}
